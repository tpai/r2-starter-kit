{
  "name": "r2-starter-kit",
  "version": "2.0.0",
  "description": "R^2 Starter Kit is an boilerplate for web development built on top of React, Redux and Webpack 4.",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "build": "rm -rf dist/ && NODE_ENV=production webpack --config webpack.config.prod.js",
    "e2e": "testcafe safari tests/e2e.js",
    "test": "NODE_ENV=test NODE_PATH=./src jest --watch",
    "test:cov": "NODE_ENV=test NODE_PATH=./src jest --coverage",
    "lint": "eslint $(find src -name \\*.js)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tpai/r2-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/tpai/r2-starter-kit/issues"
  },
  "author": "tonypai <tony77794@gmail.com> (http://github.com/tpai)",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.6",
    "connected-react-router": "^6.3.2",
    "express": "^4.16.2",
    "express-history-api-fallback": "^2.2.1",
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.8",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^22.1.3",

    "image-webpack-loader": "^4.6.0",
    "isomorphic-fetch": "^2.2.1",
    "jasmine-reporters": "^2.3.0",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.5.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "raf": "^3.4.0",
